# Production defaults for deploying against Neon Cloud
PORT=3000
NODE_ENV=production
LOG_LEVEL=info

# Set this to the Neon Cloud connection string issued for your production branch
DATABASE_URL=postgres://user:password@your-project-id.us-east-1.aws.neon.tech/appdb?sslmode=require

# Application secrets - never commit real values to source control
ARCJET_KEY=replace-with-production-arcjet-key
JWT_SECRET=replace-with-production-jwt-secret
